<cfoutput><h1>My Awesome Contacts</h1>#getPlugin("MessageBox").renderit()#<p>	<a href="#event.buildLink('contacts.add')#">[Create Contact]</a> |	<a href="#event.buildLink('api.hello')#">[Hello via REST]</a> |	<a href="#event.buildLink('api.contacts')#">[LIST via REST]</a></p><table name="contactForm" border="1" width="98%" cellpadding="5" cellspacing="1">	<thead>		<th>ID</th>		<th>Name</th>		<th>Email</th>		<th width="75">CMDS</th>	</thead>	<tbody>		<cfloop query="rc.qContacts">		<tr>			<td>#rc.qContacts.id#</td>			<td>#rc.qContacts.name#</td>			<td>#rc.qContacts.email#</td>			<td>				<a href="#event.buildLink('contacts.delete')#/id/#rc.qContacts.id#" onclick="return confirm('Are you sure?')">Delete</a>			</td>		</tr>		</cfloop>	</tbody></table></cfoutput>