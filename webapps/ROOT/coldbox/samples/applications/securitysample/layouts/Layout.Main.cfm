<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />	<title><cfoutput>#getResource("general.pageTitle")#</cfoutput></title>	<link rel="stylesheet" href="includes/css/default.css" type="text/css" />	<link rel="stylesheet" href="includes/css/list.css" type="text/css" />	<link rel="stylesheet" href="includes/css/edit.css" type="text/css" />	<link rel="stylesheet" href="includes/css/view.css" type="text/css" /></head><body>	<div id="menu">		<cfoutput>		<ul>			<li><a href="index.cfm">#getResource("general.menu.home")#</a></li>			<li>|</li>			<cfset securityManager = getPlugin('ioc').getBean('securityManager')>			<!--- Additional menu items? --->			<cfif isStruct(securityManager.getUserSession() )>				<cfset userTypeName = securityManager.getUserSession().getUserType().getName()>				<!--- Show userType related menu --->				<cfswitch expression="#userTypeName#">					<cfcase value="Administrator">						<li><a href="#event.buildLink('admin.User.Users')#">#getResource("general.menu.linkUsers")#</a></li>						<li>|</li>						<li><a href="#event.buildLink('user.Product.Products')#">#getResource("general.menu.products")#</a></li>						<li>|</li>					</cfcase>					<cfcase value="User">						<li><a href="#event.buildLink('user.Product.Products')#">#getResource("general.menu.products")#</a></li>						<li>|</li>					</cfcase>				</cfswitch>				<!--- Show logoff for both usertypes --->				<li><a class="inactive" href="#event.buildLink('Security.doLogoff')#">Logoff</a></li>			<cfelse>				<li><a href="#event.buildLink('Security.login')#">#getResource("general.menu.login")#</a></li>			</cfif>		</ul>		</cfoutput>	</div>	<div id="content">	<cfoutput>	#getPlugin("MessageBox").renderit()#	#renderView()#	</cfoutput>	</div></body></html>