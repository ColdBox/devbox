<cfoutput><h1>Create New Contact</h1>#getPlugin("MessageBox").renderit()#<form name="addContactForm" method="post" action="#event.buildLink('contacts.createContact')#">	<label>Name: 		<input type="text" name="name" size="30"/>	</label>	<br/>		<label>Email: 		<input type="text" name="email" size="30" />	</label>		<br/><br/>		<a href="#event.buildLink('contacts')#">Cancel</a> or	<input type="submit" value="Create Contact!" /></form></cfoutput>